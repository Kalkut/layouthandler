sand.define('ressources/Toggle', [
  'Seed'
], function(r) {
    
  return r.Seed.extend({
    
    onClick : function() {
      if (this._down) {
        this.up();
        this._down = false;
      }
      else {
        this.down();
        this._down = true;
      }
    },
    
    down : function()Â { //override
      
    },
    
    up : function() { //override

    }
    
  });
  
});